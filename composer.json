{
    "name": "hauerheinrich/hh-ckeditor-custom",
    "type": "typo3-cms-extension",
    "description": "Hauer-Heinrich - ckeditor custom",
    "authors": [
        {
            "name": "Christian Hackl",
            "email": "info@hauer-heinrich.de",
            "homepage": "http://www.hauer-heinrich.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Hauer-Heinrich/hh_ckeditor_custom/issues",
        "source": "https://github.com/Hauer-Heinrich/hh_ckeditor_custom",
        "docs": "https://github.com/Hauer-Heinrich/hh_ckeditor_custom/blob/master/README.md"
    },
    "version": "0.1.5",
    "license": ["GPL-2.0-or-later"],
    "keywords": [
        "typo3",
        "cms",
        "hauer-heinrich",
        "ckeditor"
    ],
    "replace": {
        "hauer-heinrich/hh_ckeditor_custom": "self.version",
        "typo3-ter/hh_ckeditor_custom": "self.version"
    },
    "require": {
        "typo3/cms-core": "^8.7.1 || ^10.4",
        "typo3/cms-rte-ckeditor": "^8.7.7 || ^10.4"
    },
    "autoload": {
        "psr-4": {
            "HauerHeinrich\\HhCkeditorCustom\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "hh_ckeditor_custom"
        }
    }
}
